{
  "name": "n8n-nodes-mixpanel",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Mixpanel providing event tracking, user analytics, cohort management, and data export capabilities.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-mixpanel.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-mixpanel#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-mixpanel/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "mixpanel",
    "analytics",
    "product-analytics",
    "event-tracking",
    "user-profiles",
    "cohorts",
    "funnels",
    "retention",
    "data-export"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MixpanelApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Mixpanel/Mixpanel.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && npm run copy:icons",
    "copy:icons": "copyfiles -u 1 \"nodes/**/*.svg\" \"nodes/**/*.png\" dist/",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
